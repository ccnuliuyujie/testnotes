<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.liuyujie.testnote"
    android:versionCode="1"
    android:versionName="1.0" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/note"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.liuyujie.testnote.AddContent"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.liuyujie.testnote.SelectAct"
            android:screenOrientation="portrait" >
        </activity>
        <activity
            android:name="com.example.liuyujie.testnote.ResultActivity"
            android:screenOrientation="portrait" >
        </activity>


    </application>

</manifest>
